<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="mj.model">

    <class name="Issue" table="Issue">
        <id name="id" column="issue_id">
            <generator class="native"/>
        </id>
        <property name="taskTitle"/>
        <property name="taskRemarks"/>
        <!--<property name="taskDate"/>-->
        <property name="isCompleted"/>
    </class>

</hibernate-mapping>